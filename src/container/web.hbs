<div>
	<main>
		<h1>Web Languages</h1>
		{{#each web.languages}}
		<h2>{{name}} ({{version}})</h2>
		<p>{{description}}</p>
		{{#if elements}}
		<h3>Elements</h3>
		<ul>
			{{#each elements}}
			<li>
				<strong>{{name}}</strong>: {{description}}
				{{#if this.attributes}}
				<ul>
					{{#each attributes}}
					<li><em>{{name}}</em>: {{description}}</li>
					{{/each}}
				</ul>
				{{/if}}
			</li>
			{{/each}}
		</ul>
		{{/if}}
		{{#if properties}}
		<h3>CSS Properties</h3>
		<ul>
			{{#each properties}}
			<li>
				<strong>{{name}}</strong>: {{description}}
				{{#if values}}
				<ul>
					{{#each values}}
					<li><em>{{value}}</em>: {{description}}</li>
					{{/each}}
				</ul>
				{{/if}}
			</li>
			{{/each}}
		</ul>
		{{/if}}
		{{#if functions}}
		<h3>JavaScript Functions</h3>
		<ul>
			{{#each this.functions}}
			<li>
				<strong>{{name}}</strong>: {{description}}
				{{#if parameters}}
				<ul>
					{{#each this.parameters}}
					<li><em>{{name}}</em>: {{type}} - {{description}}</li>
					{{/each}}
				</ul>
				{{/if}}
			</li>
			{{/each}}
		</ul>
		{{/if}}
		{{/each}}
	</main>
</div>