<main>
    <div class='card'>
        <div class='card'>
            <div class='card-body'>
                {{#with heading}}
                    {{#with main}}
                        <h1>Heading</h1>
                        <p>Paragraph</p>
                        <button class='btn btn-primary'>Primary</button>
                        <br />
                        <br />
                        <button class='btn btn-outline-primary'>Outline Primary</button>
                    {{/with}}
                {{/with}}
            </div>
        </div>
        <div class='card'>
            <div class='card-body'>
                {{#with heading}}
                    {{#with main}}
                        <h1>Heading second</h1>
                        <p>Paragraph</p>
                        <button class='btn btn-secondary'>Secondary</button>
                        <br />
                        <br />
                        <button class='btn btn-outline-secondary'>Secondary</button>
                    {{/with}}
                {{/with}}
            </div>
        </div>
        <div class='card'>
            <div class='card-body'>
                {{#with heading}}
                    {{#with main}}
                        <h1>Heading subblock</h1>
                        <p>Paragraph</p>
                        <button class='btn btn-dark'>Dark</button>
                        <br />
                        <br />
                        <button class='btn btn-outline-dark'>Dark</button>
                    {{/with}}
                {{/with}}
            </div>
        </div>
    </div>
</main>