<header>
    <section>
        {{#with navigation}}
            <div class='card mb-5'>
                <nav class='navbar'>
                    <div class='container-fluid'>
                        <span class='navbar-brand'>{{Logo}}</span>
                        <div class='nav'>
                            {{#each links}}
                                {{#if @last}}
                                    <div class='nav-item'>
                                        <span class='nav-link disabled'>
                                            {{text}}
                                        </span>
                                    </div>
                                {{else}}
                                    <div class='nav-item'>
                                        <a href='{{href}}' class='nav-link active'>
                                            {{text}}
                                        </a>
                                    </div>
                                {{/if}}
                            {{/each}}
                        </div>
                    </div>
                </nav>
            </div>
        {{/with}}
    </section>
</header>